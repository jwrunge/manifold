{
	"name": "@jwrunge/manifold",
	"version": "0.4.23",
	"tsconfig": "tsconfig.json",
	"compilerOptions": {
		"lib": ["es2022", "dom", "dom.iterable"]
	},
	"description": "A lightweight, no-build reactive state and templating library for the modern web",
	"license": "MIT",
	"exports": "./src/main.ts",
	"tasks": {
		"test": "deno test --quiet --allow-env --allow-read tests/",
		"build": "deno run --quiet --allow-read --allow-write build.ts"
	},
	"imports": {
		"@std/assert": "jsr:@std/assert@^1.0.0"
	},
	"exclude": ["node_modules/", "dist/", ".github/", "src/"]
}
