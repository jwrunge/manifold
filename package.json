{
	"name": "manifold",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"dev": "vite",
		"build": "tsc && node ./scripts/build.mjs",
		"build:debug": "MF_DEBUG_BUILD=1 tsc && MF_DEBUG_BUILD=1 node ./scripts/build.mjs",
		"build:full": "MF_VARIANT=full tsc && vite build",
		"build:light": "MF_VARIANT=light tsc && MF_LIGHT=1 vite build",
		"test": "vitest --run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@types/node": "^22.7.5",
		"esbuild": "^0.25.9",
		"jsdom": "^26.1.0",
		"typescript": "~5.8.3",
		"vite": "^7.1.2",
		"vitest": "^3.2.4"
	}
}
