{
	"$schema": "https://jsr.io/schema/config-file.v1.json",
	"name": "@jwrunge/manifold",
	"version": "0.4.23",
	"tsconfig": "tsconfig.json",
	"description": "A lightweight, no-build reactive state and templating library for the modern web",
	"compilerOptions": {
		"lib": ["es2022", "dom", "dom.iterable"]
	},
	"exports": {
		".": "./src/main.ts"
	},
	"publish": {
		"include": ["src/", "dist/", "README.md", "LICENSE"],
		"exclude": ["**/*.test.ts", "**/*.test.js"]
	}
}
